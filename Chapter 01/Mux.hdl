/** 
 * Multiplexor:
 * out = a if sel == 0
 *       b otherwise
 */

CHIP Mux {
    IN a, b, sel;
    OUT out;

    PARTS:
	Nand(a=sel,b=sel,out=sel0);

	Nand(a=a, b=sel0,out=a1);
	Nand(a=b, b=sel, out=b1);

	Nand(a=a1,b=b1,out=out);


}
